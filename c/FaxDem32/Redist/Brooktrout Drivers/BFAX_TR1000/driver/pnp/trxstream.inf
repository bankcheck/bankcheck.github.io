brktBdevpp.dll=1,trxstream\ia64

[DestinationDirs]
boston.Files.Driver.NT=10,system32\drivers
boston.Files.PropertyPage.NT=11                  ;system directory(system32)
boston.Files.Coinstaller.NT=11                   ;system directory(system32)

[boston.Files.PropertyPage.NT]
brktBdevpp.dll,,,%COPYFLG_NOSKIP%

[boston.Files.Coinstaller.NT]
brktBdevco.dll,,,%COPYFLG_NOSKIP%

[boston.Files.Driver.NT]
boston.sys,,,%COPYFLG_NOSKIP%

[TRxStream.Install.ntx86]
CopyFiles=boston.Files.Driver.NT, boston.Files.PropertyPage.NT,
AddReg= boston.PropertyPageAddReg, boston.DriverAddReg

[TRxStream.Install.ntamd64]
CopyFiles=boston.Files.Driver.NT, boston.Files.PropertyPage.NT,
AddReg= boston.PropertyPageAddReg64, boston.DriverAddReg

[TRxStream.Install.ntia64]
CopyFiles=boston.Files.Driver.NT, boston.Files.PropertyPage.NT,
AddReg= boston.PropertyPageAddReg64, boston.DriverAddReg

[TRxStream.Install.NTx86.CoInstallers]  
CopyFiles = boston.Files.Coinstaller.NT
AddReg = boston.CoinstallerAddReg

[TRxStream.Install.NTamd64.CoInstallers]  
CopyFiles = boston.Files.Coinstaller.NT
AddReg = boston.CoinstallerAddReg

[TRxStream.Install.NTia64.CoInstallers]  
CopyFiles = boston.Files.Coinstaller.NT
AddReg = boston.CoinstallerAddReg

[TRxStream.Install.NTx86.Services]
AddService = boston, %SPSVCINST_ASSOCSERVICE%, boston.Service, boston.EventLog

[TRxStream.Install.NTamd64.Services]
AddService = boston, %SPSVCINST_ASSOCSERVICE%, boston.Service, boston.EventLog

[TRxStream.Install.NTia64.Services]
AddService = boston, %SPSVCINST_ASSOCSERVICE%, boston.Service, boston.EventLog

[boston.Service]
DisplayName    = %boston.ServiceName%
ServiceType    = %SERVICE_KERNEL_DRIVER%
StartType      = %SERVICE_DEMAND_START%
ErrorControl   = %SERVICE_ERROR_NORMAL%
ServiceBinary  = %10%\system32\drivers\boston.sys

[boston.PropertyPageAddReg]
HKR,,EnumPropPages32,,"brktBdevpp.dll,EnumPropPages"
HKLM,SOFTWARE\Brooktrout Technology\DevicePropertyPage\brktBdevpp,"LaunchConfigToolOption", 0x00010001, 1
HKLM,SOFTWARE\Brooktrout Technology\DevicePropertyPage\brktBdevpp,"ConfigToolName", , "DEFAULT"
HKLM,SOFTWARE\Brooktrout Technology\DevicePropertyPage\brktBdevpp,"ConfigToolLocation", , "DEFAULT"

[boston.PropertyPageAddReg64]
HKR,,EnumPropPages32,,"brktBdevpp.dll,EnumPropPages"
HKLM,SOFTWARE\Wow6432Node\Brooktrout Technology\DevicePropertyPage\brktBdevpp,"LaunchConfigToolOption", 0x00010001, 1
HKLM,SOFTWARE\Wow6432Node\Brooktrout Technology\DevicePropertyPage\brktBdevpp,"ConfigToolName", , "DEFAULT"
HKLM,SOFTWARE\Wow6432Node\Brooktrout Technology\DevicePropertyPage\brktBdevpp,"ConfigToolLocation", , "DEFAULT"

[boston.EventLog]
AddReg = boston.EventLogAddReg

[boston.EventLogAddReg]
HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\drivers\boston.sys"
HKR,,TypesSupported,0x00010001,7 

[boston.CoinstallerAddReg]
HKR,,CoInstallers32,0x00010000,"brktBdevco.dll,BostonCoInstaller"


