[SourceDisksFiles.x86]
boston.sys=1,trxstream\x86
brktBdevco.dll=1,trxstream\x86
brktBdevpp.dll=1,trxstream\x86

[SourceDisksFiles.amd64]
boston.sys=1,trxstream\x64
brktBdevco.dll=1,trxstream\x64
brktBdevpp.dll=1,trxstream\x64

[SourceDisksFiles.ia64]
boston.sys=1,trxstream\ia64
