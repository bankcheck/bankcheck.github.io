;======================= Class Section =======================================

[ClassInstall32] 
AddReg=Brooktroutclassaddreg

[Brooktroutclassaddreg]
HKR,,,0,"ComputerTelephony"
HKR,,Icon,,-5

;======================= Custom Settings =======================================

[CoInstallerInfo]
PackageName="DEFAULT";                 Name of the Software Package to be launched from the coinstaller. This will be defaulted to "sdk_windows.exe"
PackageLocation="DEFAULT";             Location of the Software Package. This will be defaulted to "Software\Windows"
LaunchConfigTool=1;                    1 => TRUE/Lauch Configuration Tool, 0=> FALSE/Do not launch Configuration Tool
ConfigToolName="DEFAULT";              Configuration tool file name e.g configtool.exe,
ConfigToolLocation="DEFAULT";          Configuration tool location.
ConfigToolTimeout=60;                  Timeout value in seconds. Default = 60 sec.                                   
DisplayInformation="DEFAULT";          Information to be displayed by the coinstaller page.

