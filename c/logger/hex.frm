Private Sub Form_Load()
    Dim i As Integer
    Dim x As Integer
    Dim y As Integer
        
    cmdHex(0).Caption = "NULL"
    cmdHex(1).Caption = "SOH"
    cmdHex(2).Caption = "STX"
    cmdHex(3).Caption = "ETX"
    cmdHex(4).Caption = "EOT"
    cmdHex(5).Caption = "ENQ"
    cmdHex(6).Caption = "ACK"
    cmdHex(7).Caption = "BEL"
    cmdHex(8).Caption = "BS"
    cmdHex(9).Caption = "HT"
    cmdHex(10).Caption = "LF"
    cmdHex(11).Caption = "VT"
    cmdHex(12).Caption = "FF"
    cmdHex(13).Caption = "CR"
    cmdHex(14).Caption = "SO"
    cmdHex(15).Caption = "SI"
    cmdHex(16).Caption = "DLE"
    cmdHex(17).Caption = "DC1"
    cmdHex(18).Caption = "DC2"
    cmdHex(19).Caption = "DC3"
    cmdHex(20).Caption = "DC4"
    cmdHex(21).Caption = "NAK"
    cmdHex(22).Caption = "SYN"
    cmdHex(23).Caption = "ETB"
    cmdHex(24).Caption = "CAN"
    cmdHex(25).Caption = "EM"
    cmdHex(26).Caption = "SUB"
    cmdHex(27).Caption = "ESC"
    cmdHex(28).Caption = "FS"
    cmdHex(29).Caption = "GS"
    cmdHex(30).Caption = "RS"
    cmdHex(31).Caption = "US"
        
    x = 100
    y = 0
    
    For i = cmdHex.LBound To cmdHex.UBound
        If i Mod 8 = 0 Then
            x = 100
            y = y + 400
        Else
            x = x + 900
        End If
        
        cmdHex(i).Left = x
        cmdHex(i).Top = y
        cmdHex(i).Width = 800
        cmdHex(i).Caption = cmdHex(i).Caption & " (" & i & ")"
    Next
        
End Sub
