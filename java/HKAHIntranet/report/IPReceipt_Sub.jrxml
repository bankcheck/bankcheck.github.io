	<queryString>
		<![CDATA[SELECT TB.STNTDATE, SUM(TB.STNNAMT) AMOUNT, TB.DESCRIPTION
      FROM (SELECT TO_CHAR(ST.STNTDATE, 'dd-mm-yyyy') STNTDATE,
                   ST.STNNAMT,
                   DECODE(ST.STNRLVL,
                          1,
                          DPS.DSCDESC,
                          2,
                          DP.DPTNAME,
                          3,
                          ST.STNDESC,
                          4,
                          PK.PKGNAME,
                          NULL) DESCRIPTION
              FROM SLIPTX ST, DPSERV DPS, DEPT DP, PACKAGE PK
             WHERE ST.DSCCODE = DPS.DSCCODE(+)
               AND SUBSTR(ST.GLCCODE, 1, 4) = DP.DPTCODE(+)
               AND ST.PKGCODE = PK.PKGCODE(+)
               AND ST.SLPNO IN ('20092770243','20092770244' )
               AND ST.STNSTS IN ('N','A')
               AND ST.STNTYPE IN ('S', 'R')
               AND ST.STNRLVL <> 3) TB
     GROUP BY TB.STNTDATE, TB.DESCRIPTION
    UNION ALL
    SELECT *
      FROM (SELECT TO_CHAR(ST.STNTDATE, 'dd-mm-yyyy') STNTDATE,
                   ST.STNNAMT,
                   DECODE(ST.STNRLVL,
                          1,
                          DPS.DSCDESC,
                          2,
                          DP.DPTNAME,
                          3,
                          ST.STNDESC,
                          4,
                          PK.PKGNAME,
                          NULL) DESCRIPTION
              FROM SLIPTX ST, DPSERV DPS, DEPT DP, PACKAGE PK
             WHERE ST.DSCCODE = DPS.DSCCODE(+)
               AND SUBSTR(ST.GLCCODE, 1, 4) = DP.DPTCODE(+)
               AND ST.PKGCODE = PK.PKGCODE(+)
               AND ST.SLPNO IN ('20092770243','20092770244' )
               AND ST.STNSTS IN ('N', 'A')
               AND ST.STNTYPE IN ('S', 'R')
               AND ST.STNRLVL = 3) TB2]]>
	</queryString>
