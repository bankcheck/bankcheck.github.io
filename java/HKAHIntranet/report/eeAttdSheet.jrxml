	<queryString>
		<![CDATA[select ce.co_event_id, c.co_event_desc, c.co_event_short_desc, cs.co_schedule_desc,
       CE.CO_SCHEDULE_ID, CE.CO_ENROLL_ID, cs.co_schedule_start, cs.co_schedule_end,
       cs.co_schedule_duration,
       CS.CO_LOCATION_ID, L.CO_LOCATION_DESC, CS.CO_LOCATION_DESC CO_LOCATION_OTHER, CS.CO_LECTURE_DESC, CS.CO_SCHEDULE_SIZE,
       ce.co_attend_date,
       CS.CO_SCHEDULE_SIZE, CS.CO_SCHEDULE_ENROLLED, CS.CO_SCHEDULE_STATUS, C.CO_REQUIRE_ASSESSMENT_PASS,
-- TWAH use S.CO_DISPLAY_NAME, HKAH use S.CO_STAFFNAME
       CE.CO_ATTEND_STATUS, S.CO_STAFF_ID, S.CO_STAFFNAME, S.CO_DISPLAY_NAME, S.CO_POSITION_1, S.CO_POSITION_2,
       S.CO_DEPARTMENT_CODE, D.CO_DEPARTMENT_DESC,
       ce.co_assessment_pass_date,
       CE.CO_ATTEND_DATE2,CE.CO_REMARK,CS.CO_SCHEDULE_DESC,
case when (select count(1) from co_department_event where co_module_code='staffCat' and co_department_code = S.CO_DEPARTMENT_CODE and instr(co_remarks, trim(S.CO_POSITION_1 || ' ' || S.CO_POSITION_2)) > 0) > 0 then
        (
            select de.co_event_id
            from co_department_event de
            where co_module_code='staffCat' and co_department_code = S.CO_DEPARTMENT_CODE
            and instr(de.co_remarks, trim(S.CO_POSITION_1 || ' ' || S.CO_POSITION_2)) > 0
            and rownum <= 1
        )
when (select count(1) from co_department_event where co_module_code='staffCat' and co_department_code = S.CO_DEPARTMENT_CODE and instr(co_remarks, trim(s.CO_JOB_DESCRIPTION)) > 0) > 0 then
        (
            select de.co_event_id
            from co_department_event de
            where co_module_code='staffCat' and co_department_code = S.CO_DEPARTMENT_CODE
            and instr(de.co_remarks, trim(s.CO_JOB_DESCRIPTION)) > 0
            and rownum <= 1
        )
        else
        (
            select de.co_event_id
            from co_department_event de
            where co_module_code='staffCat' and co_department_code = S.CO_DEPARTMENT_CODE
            and de.co_remarks is null
            and rownum <= 1
        )
        end category_event, s.CO_JOB_DESCRIPTION, CE.CO_LUNCH,  CS.CO_LUNCH_AVA, CE.CO_REMARK2, S.CO_LASTNAME || ' ' || S.CO_FIRSTNAME as CO_FULLNAME
FROM   CO_ENROLLMENT CE, CO_EVENT C, CO_SCHEDULE CS, CO_STAFFS S, CO_DEPARTMENTS D, CO_LOCATION L
WHERE  CE.CO_SITE_CODE = C.CO_SITE_CODE
AND    CE.CO_MODULE_CODE = C.CO_MODULE_CODE
and    ce.co_event_id = c.co_event_id
AND    CE.CO_MODULE_CODE = $P{module}
AND    CE.CO_SITE_CODE = CS.CO_SITE_CODE (+)
AND    CE.CO_MODULE_CODE = CS.CO_MODULE_CODE (+)
AND    CE.CO_EVENT_ID = CS.CO_EVENT_ID (+)
AND    CE.CO_SCHEDULE_ID = CS.CO_SCHEDULE_ID (+)
AND    CE.CO_USER_ID = S.CO_STAFF_ID (+)
and    s.co_department_code = d.co_department_code (+)
and    cs.co_site_code = l.co_site_code (+)
AND    cS.CO_LOCATION_ID = L.CO_LOCATION_ID (+)
and    ce.co_event_id = $P{eventID}
AND    CE.CO_SCHEDULE_ID = $P{scheduleID}
-- only show 3 year history
AND    CE.CO_MODIFIED_DATE >= SYSDATE - 1095
AND    CE.CO_ENABLED = 1
$P!{orderByP}]]>
	</queryString>
