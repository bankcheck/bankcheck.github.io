<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="RptTreatByPatient" pageWidth="595" pageHeight="842" columnWidth="595" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="DATERANGE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
SLP.PATNO,
(
SELECT
DECODE(SIGN(INSTR(P.TITDESC,'.', 1)),0,P.TITDESC,1,SUBSTR(P.TITDESC,LENGTH(P.TITDESC)*-1,INSTR(P.TITDESC,'.', 1)-1))||'. '||P.PATGNAME||', '||P.PATFNAME
FROM PATIENT P WHERE P.PATNO = SLP.PATNO
) AS PATNAME,
SPX.ITMCODE,
(SELECT ITEM.ITMNAME FROM ITEM WHERE ITEM.ITMCODE = SPX.ITMCODE) AS ITMNAME,
SUM(SPX.STNNAMT) AS AMT,
COUNT(
  CASE WHEN
    SPX.STNSTS = 'N'
  THEN SPX.ITMCODE ELSE NULL END) AS COUNT_TREAT
FROM SLIPTX SPX, SLIP SLP
WHERE SLP.SLPNO = SPX.SLPNO
AND SLP.PATNO = '135375'
AND SPX.GLCCODE LIKE '210-%'
AND SPX.STNTDATE >= TO_DATE('01/04/2013 00:00', 'dd/mm/yyyy hh24:mi')
AND SPX.STNTDATE < TO_DATE('30/04/2013 00:00', 'dd/mm/yyyy hh24:mi') + 1
GROUP BY SLP.PATNO , SPX.ITMCODE
HAVING COUNT(
  CASE WHEN
    SPX.STNSTS = 'N'
  THEN SPX.ITMCODE ELSE NULL END) > 0
ORDER BY SPX.ITMCODE ASC]]>
	</queryString>
