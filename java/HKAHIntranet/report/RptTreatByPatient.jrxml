	<field name="PATNAME" class="java.lang.String"/>
	<field name="ITMCODE" class="java.lang.String"/>
	<field name="ITMNAME" class="java.lang.String"/>
	<field name="AMT" class="java.lang.String"/>
	<field name="COUNT_TREAT" class="java.lang.String"/>
