	</variable>
	<variable name="V_AMT" class="java.math.BigDecimal">
		<variableExpression><![CDATA[new BigDecimal($F{AMT})]]></variableExpression>
	</variable>
	<variable name="SUMTOTAL" class="java.math.BigDecimal" resetType="Group" resetGroup="PATNO" calculation="Sum">
