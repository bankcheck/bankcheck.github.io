	<parameter name="DateStr" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
  W.WRDNAME "WARD",
  R.PATNO,
  TO_CHAR(R.REGID) "REGID",
  TO_CHAR(R.REGDATE, 'YYYY/MM/DD HH24:MI:SS') "ADM_DATE",
  TO_CHAR(I.INPDDATE, 'YYYY/MM/DD HH24:MI:SS') "DISCH_DATE",
  TO_CHAR(TRUNC(I.INPDDATE) - TRUNC(R.REGDATE) + 1) "LOS",
  I.BEDCODE,
  R.REGSTS
FROM
  INPAT I LEFT JOIN REG R ON I.INPID = R.INPID
  LEFT JOIN BED B ON I.BEDCODE = B.BEDCODE
  LEFT JOIN ROOM RM ON B.ROMCODE = RM.ROMCODE
  LEFT JOIN WARD W ON RM.WRDCODE = W.WRDCODE
WHERE
  I.INPDDATE >= TO_DATE($P{DateStr}, 'DD/MM/YYYY HH24:MI:SS')
  AND I.INPDDATE < ADD_MONTHS(TO_DATE($P{DateStr}, 'DD/MM/YYYY HH24:MI:SS'), 1)
  AND R.REGTYPE = 'I'
  AND R.REGSTS = 'N'
ORDER BY W.WRDNAME, R.REGDATE, R.PATNO]]>
	</queryString>
