String listLabel = "function.lmcedm.sendmail";
boolean sendEmailAction = false;
if ("sendemail".equals(command)) {
	sendEmailAction = true;
}

try {
	if ("1".equals(step)) {
		if (sendEmailAction) {
			String content = null;
			String subject = null;
			if ("en".equals(lang)) {
				content = getMailContentEng();
				subject =  "HKAH Adventist Hospital - LMC EDM";
			} else if ("tc".equals(lang)) {
				content = getMailContentTc();
				subject =  " - LMC EDM";
			}

			boolean success = UtilMail.sendMail(
					ConstantsServerSide.MAIL_ALERT, emailTo,
