	<queryString>
		<![CDATA[SELECT
         cs.usrid,
         ct.ctxsno,
         ct.ctxname,
         ct.ctxamt,
         ct.slpno as ctxdesc,
         to_char(ct.ctxcdate,'dd/mm/yyyy')ctxcdate,
        decode(ct.ctxmeth,'C','Cash','D','Credit Card','E','EPS','Q','Cheque','A','Autopay','U','CUP','T','OCTOPUS') as ctxmeth,
         sit.stename,
         cd.ctnctype,
        decode(Nvl(r.regtype, 'O'),'D','Day Case','I','In-Patient','O','Out-Patient','Z','Non-Patient') as regtype
      FROM  cashtx ct, cashier cs, site sit, cardtx cd, reg r,slip s
      WHERE  ct.ctxcdate >= to_date('01/01/2006', 'DD/MM/YYYY')
             and ct.ctxcdate < to_date('02/01/2006', 'DD/MM/YYYY')+1
             and ct.slpno = s.slpno
             and s.regid = r.regid (+)
             and ct.ctxtype = 'R'
             and ct.ctxcat = 'P'
             and ct.ctxsts IN ('N', 'V', 'R')
             and ct.ctnid = cd.ctnid(+)
             and ct.cshcode = cs.cshcode
             and cs.stecode= 'HKAH'
             and cs.stecode = sit.stecode]]>
	</queryString>
