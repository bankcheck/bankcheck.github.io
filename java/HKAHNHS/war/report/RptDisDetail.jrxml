	<queryString>
		<![CDATA[SELECT sk.sckcode,
       sk.sckdesc,
       sum(decode(ds.destype,'H',1,0)) as cntHome,
       sum(decode(ds.destype,'P',1,0)) as cntHospital,
       sum(decode(ds.destype,'D',1,0)) as cntDeath,
       sd.SDSCODE, sd.SDSDESC
FROM   inpat ip,
       dest ds,
        sick sk,
       sdisease sd,
       reg rg
WHERE  ip.inpddate>= to_date($P{StartDate}, 'dd-mm-yyyy')
       and ip.inpddate< to_date($P{EndDate}, 'dd-mm-yyyy') + 1
       and ip.inpddate is not null
       and ip.inpid = rg.inpid
       and rg.regsts = 'N'
       and rg.stecode= $P{SteCode}
       and ip.sdscode  = sd.sdscode
       and ip.descode = ds.descode
       and sk.sckcode = sd.sckcode
       and sd.sckcode = 'DH'
       and sd.SDSNEW = -1
GROUP BY
      sk.sckcode,
      sk.sckdesc,
      sd.SDSCODE,
      sd.SDSDESC]]>
	</queryString>
