		<![CDATA[SELECT
         st.itmcode, it.itmname, st.pkgcode, st.stnsts,
         st.stnbamt, st.stnoamt, st.slpno, st.doccode,
         st.usrid, sc.stename,to_char(st.stncdate,'dd/mm/yyyy')stncdate
    FROM sliptx st, item it, slip sp, site sc
        WHERE
                    st.itmcode = it.itmcode and
                    st.slpno = sp.slpno and
                    sp.stecode = sc.stecode and
                    sp.stecode= 'HKAH' and
                     (st.stncdate >= to_date('01/01/2006', 'dd/mm/yyyy')
                    and st.stncdate< to_date('01/02/2006', 'dd/mm/yyyy')+1 )and
                    (st.stnsts= 'N'  or st.stnsts= 'A') and
                    stnbamt <> stnoamt and
                    it.itmpoverrd = -1
                    order by it.itmcode]]>
	</queryString>
