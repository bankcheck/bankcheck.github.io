	<queryString>
		<![CDATA[SELECT
     S."SLPNO" AS SLPNO,
     P."PATNO",
     DECODE(S.SLPFNAME,NULL,P.PATFNAME||' '||P.PATGNAME,S.SLPFNAME||' '||S.SLPGNAME)AS PATNAME,
     P."PATCNAME",
     D.DOCFNAME||' '||D.DOCGNAME AS DOCNAME,
     D."DOCCNAME"
FROM
     "SLIP" S,
     "PATIENT" P,
     "DOCTOR" D
WHERE S.PATNO = P.PATNO(+)
 AND  S.DOCCODE = D.DOCCODE(+)
 AND  S.SLPNO = $P{P_SLPNO}]]>
