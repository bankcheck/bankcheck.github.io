	<queryString>
		<![CDATA[select * from
(SELECT  sc.stename,
                 decode(st.stnsts,'U','Cancelled Items','Transferred Items') stnsts
                 ,decode(sp.slptype,'O','Out Patient','I','In Patient','Day Case') slptype,
                 trunc(st.stntdate) stntdate, st.glccode,
                 sp.slpfname, sp.slpgname, pt.patfname, sp.patno, pt.patgname,
                 st.slpno, st.itmcode, st.stndesc,
                 (-1 * st.stnnamt) as stnnamt, st.usrid
        FROM  sliptx st,
               slip sp,
               patient pt,
               site sc
        WHERE (st.stncdate between to_date('01/01/2006', 'DD/MM/YYYY')
                    and to_date('05/01/2006', 'DD/MM/YYYY')+1) and
                    st.stnsts = 'T' and
                    sp.stecode= 'HKAH' and
                    st.slpno = sp.slpno and
                    sp.stecode = sc.stecode and
                    sp.patno = pt.patno(+)
union all
SELECT           sc.stename,
                 decode(st.stnsts,'U','Cancelled Items','Transferred Items') stnsts,
                 decode(sp.slptype,'O','Out Patient','I','In Patient','Day Case') slptype,
                 trunc(st.stntdate) stntdate, st.glccode,
                 sp.slpfname, sp.slpgname, pt.patfname, sp.patno, pt.patgname,
                 st.slpno, st.itmcode, b.stndesc,
                 (-1 * st.stnnamt) as stnnamt, st.usrid
        FROM  sliptx st,
               sliptx b,
               slip sp,
               patient pt,
               site sc
        WHERE  (st.stncdate between to_date('01/01/2006', 'DD/MM/YYYY')
                    and to_date('05/01/2006', 'DD/MM/YYYY')+1) and
                    st.stnsts = 'U' and
                    sp.stecode= 'HKAH' and
                    st.slpno = sp.slpno and
                    st.slpno = b.slpno and
                    st.stndesc = b.stnseq and
                    sp.stecode = sc.stecode and
                    sp.patno = pt.patno(+))
                    order by
                    stnsts,slptype,
                    stntdate,glccode,slpno]]>
	</queryString>
