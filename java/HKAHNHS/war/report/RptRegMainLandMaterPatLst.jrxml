	<queryString language="SQL">
		<![CDATA[SELECT s.slpNo,
       p.patFname|| ' ' || p.patGname AS EName,
       SUBSTR(p.patidno,-4) AS patId,
       TRUNC(p.patbdate) AS patbdate,
       s.edc
FROM slip s,
     patient p
Where s.PatNo = p.PatNo
      AND s.firstprtdt>=TO_DATE($P{StartDate},'dd/mm/yyyy')
      AND s.firstprtdt<TO_DATE($P{EndDate},'dd/mm/yyyy')+1
      AND s.steCode=$P{SteCode}
order by
      edc,
      EName]]>
