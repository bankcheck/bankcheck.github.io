	<queryString>
		<![CDATA[SELECT
     S."SLPNO" AS SLPNO,
     P."PATNO",
     DECODE(S.SLPFNAME,NULL,P.PATFNAME||' '||P.PATGNAME,S.SLPFNAME||' '||S.SLPGNAME)||'   '||P."PATCNAME" AS PATNAME,
     R."REGDATE",
     TO_CHAR(R.REGDATE,'HH24:MI')AS REGTIME,
     I.BEDCODE||'-'||I.ACMCODE AS BEDACM,
     I."INPDDATE",
     TO_CHAR(I.INPDDATE,'HH24:MI')AS INPDTIME,
     D.DOCFNAME||' '||D.DOCGNAME||'   '||D."DOCCNAME" AS DOCNAME
FROM
     "SLIP" S,
     "PATIENT" P,
     "REG" R,
     "INPAT" I,
     "DOCTOR" D
WHERE
     S.REGID = R.REGID
 AND S.PATNO = P.PATNO(+)
 AND R.INPID = I.INPID(+)
 AND S.DOCCODE = D.DOCCODE
 AND S.SLPNO = $P{P_SLPNO}
GROUP BY
	S.SLPFNAME, S.SLPGNAME, P.PATFNAME, P.PATGNAME, P.PATCNAME, P.PATNO,
	R.REGDATE, I.BEDCODE, I.ACMCODE, I.INPDDATE, D.DOCFNAME, D.DOCGNAME,
	D.DOCCNAME]]>
	</queryString>
