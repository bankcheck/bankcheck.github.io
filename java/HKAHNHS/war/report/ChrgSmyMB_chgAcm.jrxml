	<queryString>
		<![CDATA[SELECT * FROM (
SELECT 'Mother Information' AS MB,
     S.SLPNO AS SLPNO,
     P."PATNO",
     DECODE(S.SLPFNAME,NULL,P.PATFNAME||' '||P.PATGNAME,S.SLPFNAME||' '||S.SLPGNAME)||'   '||P."PATCNAME" AS PATNAME,
     R."REGDATE",
     TO_CHAR(R.REGDATE,'HH24:MI') AS REGTIME,
     I.BEDCODE||'-'||I.ACMCODE AS BEDACM,
     I."INPDDATE",
     TO_CHAR(I.INPDDATE,'HH24:MI') AS INPDTIME,
     D.DOCFNAME||' '||D.DOCGNAME||'   '||D.DOCCNAME AS DOCNAME
FROM
     "SLIP" S,
     "PATIENT" P,
     "REG" R,
     "INPAT" I,
     "DOCTOR" D
WHERE
     S.REGID = R.REGID
 AND S.PATNO = P.PATNO(+)
 AND R.INPID = I.INPID(+)
 AND S.DOCCODE = D.DOCCODE
 AND S.SLPNO = $P{P_SLPNO}
 UNION
 SELECT 'Baby Information' AS MB,
     S.SLPNO AS SLPNO,
     P."PATNO",
     DECODE(S.SLPFNAME,NULL,P.PATFNAME||' '||P.PATGNAME,S.SLPFNAME||' '||S.SLPGNAME)||'   '||P."PATCNAME" AS PATNAME,
     R."REGDATE",
     TO_CHAR(R.REGDATE,'HH24:MI') AS REGTIME,
     I.BEDCODE||'-'||I.ACMCODE AS BEDACM,
     I."INPDDATE",
     TO_CHAR(I.INPDDATE,'HH24:MI') AS INPDTIME,
     D.DOCFNAME||' '||D.DOCGNAME||'   '||D.DOCCNAME AS DOCNAME
FROM
     "SLIP" S,
     "PATIENT" P,
     "REG" R,
     "INPAT" I,
     "DOCTOR" D
WHERE
     S.REGID = R.REGID
 AND S.PATNO = P.PATNO(+)
 AND R.INPID = I.INPID(+)
 AND S.DOCCODE = D.DOCCODE
 AND S.SLPNO IN ($P!{ALLSLPNO})
 AND S.SLPNO <> $P{P_SLPNO})
ORDER BY MB DESC]]>
	</queryString>
