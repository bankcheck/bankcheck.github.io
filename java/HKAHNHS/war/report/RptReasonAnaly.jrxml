	<queryString language="SQL">
		<![CDATA[SELECT
      sc.stename,
      rs.rsncode,
      rs.rsndesc,
      ds.destype, count(*) as cnt,
      to_date($P{StartDate},'dd-mm-yyyy') as DateRangeStart,
      to_date($P{EndDate}, 'dd-mm-yyyy') as DateRangeEnd
FROM
    inpat ip,
    reg rg,
    sreason sr,
    reason rs,
    dest ds,
    site sc
WHERE
     ip.inpddate>= to_date($P{StartDate}, 'dd-mm-yyyy')
     and inpddate< to_date($P{EndDate}, 'dd-mm-yyyy') + 1
     and ip.inpddate is not null
     and ip.inpid = rg.inpid
     and rg.regsts= 'N'
     and rg.stecode= $P{SteCode}
     and ip.rsncode  = sr.srsncode
     and sr.rsncode = rs.rsncode
     and ip.descode = ds.descode
     and rg.stecode = sc.stecode
GROUP BY
       sc.stename,
       rs.rsncode,
       rs.rsndesc,
       ds.destype]]>
