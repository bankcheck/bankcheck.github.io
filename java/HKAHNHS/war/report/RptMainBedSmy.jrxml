	<queryString>
		<![CDATA[SELECT ward, room, acmcode, type,noofbed,noofday
 FROM(
select
ward, room, acmcode, type, noofbed, Sum (noofday) noofday
From
(
select a.wrdcode ward,a.romcode room,a.acmcode acmcode,a.bedoff type, a.days noofday, count (*) as noofbed
             From (
                  select
                     a.wrdcode,a.romcode,a.acmcode,a.bedoff, sum(a.days) as days
                  from (
                           SELECT
                              r.wrdcode, b.romcode, r.acmcode, b.bedoff, count(*) as temp  ,
                             sum(to_date(to_char(bh.bhsedate,'DD/MM/YYYY')|| ' 00:00','DD/MM/YYYY HH24:MI:SS') - to_date(to_char(bh.bhsdate,'DD/MM/YYYY')|| ' 00:00','DD/MM/YYYY HH24:MI:SS')) as days
                           From
                               bedhist bh, bed b, room r, reg rg
                           Where
                               bh.bhsdate > = to_date ('10/02/2003','DD/MM/YYYY')
                               and bh.bhsedate < to_date ('10/08/2004','DD/MM/YYYY') + 1
                               and bh.bhsedate is not null
                              AND b.bedcode= bh.bedcode
                              AND b.stecode= 'HKAH'
                               AND b.romcode= r.romcode
                               and rg.regsts = 'N'
                               and rg.regtype = 'I'
                               and bh.regid = rg.regid
                           Group By
                               r.wrdcode , b.romcode, b.bedoff, r.AcmCode
                           Union

                        SELECT
                               r.wrdcode, b.romcode, r.acmcode, b.bedoff, count(*) as temp  ,
                              sum(to_date('10/08/2004','DD/MM/YYYY') - to_date(to_char(bh.bhsdate,'DD/MM/YYYY')|| ' 00:00','DD/MM/YYYY HH24:MI:SS')) as days
                          From
                               bedhist bh, bed b, room r, reg rg
                           Where
                               bh.bhsdate > = to_date ('10/02/2003','DD/MM/YYYY')
                               and bh.bhsdate <  to_date ('10/08/2004','DD/MM/YYYY') + 1
                               and bh.bhsedate is null
                               AND b.bedcode= bh.bedcode
                               AND b.stecode= 'HKAH'
                              AND b.romcode= r.romcode
                               and rg.regsts = 'N'
                               and rg.regtype = 'I'
                               and bh.regid = rg.regid
                           Group By
                               r.wrdcode , b.romcode, b.bedoff, r.AcmCode
                           Union
                  SELECT
                               r.wrdcode, b.romcode, r.acmcode, b.bedoff, count(*) as temp  ,
                              sum(to_date( '10/08/2004', 'DD-MM-YYYY') -  to_date(to_char(bh.bhsdate,'DD/MM/YYYY')|| ' 00:00','DD/MM/YYYY HH24:MI:SS')) as days
                           From
                               bedhist bh, bed b, room r, reg rg
                           Where
                               bh.bhsdate > = to_date ('10/02/2003','DD/MM/YYYY')
                               and bh.bhsdate < to_date ('10/08/2004','DD/MM/YYYY') + 1
                               and bh.bhsedate >= to_date ('10/08/2004','DD/MM/YYYY') + 1
                               and bh.bhsedate is not null
                               AND b.bedcode= bh.bedcode
                               AND b.stecode= 'HKAH'
                               AND b.romcode= r.romcode
                               and rg.regsts = 'N'
                               and rg.regtype = 'I'
                               and bh.regid = rg.regid
                           Group By
                               r.wrdcode , b.romcode, b.bedoff, r.AcmCode
                           Union

               SELECT
                             r.wrdcode, b.romcode, r.acmcode, b.bedoff, count(*) as temp  ,
                              sum(to_date(to_char(bh.bhsedate,'DD/MM/YYYY')|| ' 00:00','DD/MM/YYYY HH24:MI:SS') - to_date('10/02/2003', 'DD-MM-YYYY')) as days
                           From
                               bedhist bh, bed b, room r, reg rg
                           Where
                               bh.bhsdate < to_date ('10/02/2003','DD/MM/YYYY')
                               and bh.bhsedate >= to_date ('10/02/2003','DD/MM/YYYY')
                               and bh.bhsedate < to_date ('10/08/2004','DD/MM/YYYY') + 1
                               and bh.bhsedate is not null
                               AND b.bedcode= bh.bedcode
                               AND b.stecode= 'HKAH'
                               AND b.romcode= r.romcode
                               and rg.regsts = 'N'
                               and rg.regtype = 'I'
                               and bh.regid = rg.regid
                           Group By
                               r.wrdcode , b.romcode, b.bedoff, r.AcmCode
                           Union

                          SELECT
                              r.wrdcode, b.romcode, r.acmcode, b.bedoff, count(*) as temp  ,
                              sum(to_date( '10/08/2004', 'DD-MM-YYYY') - to_date('10/02/2003','DD/MM/YYYY')) as days
                          From
                               bedhist bh, bed b, room r, reg rg
                           Where
                               bh.bhsdate < to_date ('10/02/2003','DD/MM/YYYY')
                               and bh.bhsedate is null
                               AND b.bedcode= bh.bedcode
                               AND b.stecode= 'HKAH'
                               AND b.romcode= r.romcode
                               and rg.regsts = 'N'
                               and rg.regtype = 'I'
                               and bh.regid = rg.regid
                           Group By
                               r.wrdcode , b.romcode, b.bedoff, r.AcmCode
                           Union

                           SELECT
                              r.wrdcode, b.romcode, r.acmcode, b.bedoff, count(*) as temp  ,
                               sum(to_date( '10/08/2004', 'DD-MM-YYYY') - to_date( '10/02/2003', 'DD-MM-YYYY')) as days
                           From
                              bedhist bh, bed b, room r, reg rg
                           Where
                               bh.bhsdate < to_date ('10/02/2003','DD/MM/YYYY')
                               and bh.bhsedate >  to_date ('10/08/2004','DD/MM/YYYY')
                               and bh.bhsedate is not null
                               AND b.bedcode= bh.bedcode
                               AND b.stecode= 'HKAH'
                               AND b.romcode= r.romcode
                               and rg.regsts = 'N'
                               and rg.regtype = 'I'
                               and bh.regid = rg.regid
                           Group By
                               r.wrdcode , b.romcode, b.bedoff, r.AcmCode

                      ) a
               Group By
                   a.wrdcode , a.romcode, a.AcmCode, a.bedoff
             ) a, room r , bed b
             Where
               a.romcode = b.romcode
               And b.romcode = r.romcode
             Group By
               a.wrdcode , a.romcode, a.AcmCode, a.bedoff, a.Days
)
Where type<>0
Group By ward, room, acmcode, type, noofbed
Order by ward, room, acmcode, type
)
 ORDER BY ward]]>
